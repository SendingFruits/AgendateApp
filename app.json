{
  "expo": {
    "name": "agendate-app",
    "slug": "agendate-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "runtimeVersion": "1.0.0",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Necesita usar la ubiación del dispositivo.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Necesita usar la ubiación del dispositivo."
      }
    },
    "android": {
      "permissions": [
        "ACCESS_BACKGROUND_LOCATION",
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.FOREGROUND_SERVICE"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ethelvan.agendateapp"
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Permite a $(PRODUCT_NAME) utilizar tu ubicación."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a44a7704-4472-41f9-ac23-83f6f82d9de0"
      }
    },
    "owner": "ethelvan",
    "updates": {
      "url": "https://u.expo.dev/a44a7704-4472-41f9-ac23-83f6f82d9de0"
    }
  }
}
